PORT=5000

# Comma separated list of provider endpoints (will be tried in order).
# Note: endpoints may include '?q=' or 'image=' parts. Example using your given providers:
PROVIDERS=https://apis-keith.vercel.app/keithai?q=,https://apis-keith.vercel.app/ai/gpt?q=,https://apis-keith.vercel.app/ai/claudeai?q=,https://apis-keith.vercel.app/ai/mistral?q=,https://apis-keith.vercel.app/ai/gpt41Nano?q=,https://apis-keith.vercel.app/ai/bard?q=,https://apis-keith.vercel.app/ai/perplexity?q=,https://apis-keith.vercel.app/ai/o3?q=,https://apis-keith.vercel.app/ai/chatgpt4?q=,https://apis-keith.vercel.app/ai/venice?q=,https://apis-keith.vercel.app/ai/codegen?q=,https://apis-keith.vercel.app/ai/deepseekV3?q=,https://apis-keith.vercel.app/ai/lyricsgen?topic=Heartbreak&genre=blues&mood=sad&structure=verse_chorus_bridge&language=en,https://apis-keith.vercel.app/ai/gpt4?q=,https://apis-keith.vercel.app/ai/qwenai?q=,https://apis-keith.vercel.app/ai/gemini-vision2?image=,https://apis-keith.vercel.app/ai/gemini-vision?image=

# Optional per-provider API key mapping.
# Format: provider_url_prefix|API_KEY, separated by commas.
# Example:
# KEY_MAP=https://apis-keith.vercel.app/ai/gpt|MY_KEY_FOR_GPT,https://apis-keith.vercel.app/ai/claudeai|MY_CLAUDE_KEY
KEY_MAP=

# Timeout per provider (ms)
PROVIDER_TIMEOUT=7000

# How many attempts per provider (retries)
PROVIDER_ATTEMPTS=2

# Cache TTL in seconds
CACHE_TTL=30
